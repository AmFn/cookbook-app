        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TestActivity"></activity>
        <activity android:name=".activity.CreateWebActivity" />
        <activity android:name=".activity.MoreFoodActivity" />
        <activity android:name=".activity.MyFoodActivity" />
        <activity android:name=".activity.UserHomeActivity" />
